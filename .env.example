# Hotel Reception App Environment Configuration Template
# Copy this file to .env and update the values

# Admin Authentication - Change this password!
REACT_APP_ADMIN_CODE=your-secure-admin-password

# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_API_TIMEOUT=30000

# Feature Flags
REACT_APP_ENABLE_LOGGING=true
REACT_APP_ENABLE_DEBUGGING=false

# Hotel Configuration
REACT_APP_HOTEL_NAME=Your Hotel Name
REACT_APP_HOTEL_VERSION=1.1.0